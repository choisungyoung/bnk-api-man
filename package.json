    "sqlite3": "^5.0.2",
    "tui-grid": "^4.17.3",
    "vue": "^2.6.14",
    "vue-code-highlight": "^0.7.8",
    "vue-cookies": "^1.7.4",
    "vue-loading-overlay": "^3.0.1",
    "vue-prism-editor": "^1.2.2",
    "vue-toasted": "^1.1.28",
    "vuedraggable": "^2.24.3",
    "vuetify": "^2.4.0",
    "vuetify-dialog": "^2.0.16",
    "vuex": "^3.6.2",
    "webpack-node-externals": "^2.5.2"
